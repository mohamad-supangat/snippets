{
  "echo __ function": {
    "prefix": "echo __",
    "body": ["{{ __('${1}') }}"],
    "description": "__ function echo"
  },
  "echo translate function": {
    "prefix": "echo translate",
    "body": ["{{ translate('${1}') }}"],
    "description": "translate function echo"
  },
  "required": {
    "prefix": "req",
    "body": ["required ${1}"]
  },
  "wire:model": {
    "prefix": "wire:model",
    "body": ["wire:model=\"${1}\""]
  },
  "label with lang": {
    "prefix": "label",
    "body": [":label=\"__('$1')\""]
  }
}
