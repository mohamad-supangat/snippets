{
  "name": "snippsets",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "javascript",
          "javascriptreact",
          "vue",
          "svelte"
        ],
        "path": "./snippets/javascript.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue.json"
      },
      {
        "language": "php",
        "path": "./snippets/php.json"
      },
      {
        "language": "xml",
        "path": "./snippets/xml.json"
      },
      {
        "language": "blade",
        "path": "./snippets/laravel/snippets.json"
      },
      {
        "language": "blade",
        "path": "./snippets/laravel/helpers.json"
      },
      {
        "language": "blade",
        "path": "./snippets/laravel/livewire.json"
      },
      {
        "language": "php",
        "path": "./snippets/laravel/blade.json"
      }
    ]
  }
}
